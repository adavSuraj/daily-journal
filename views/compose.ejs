<%- include('partials/header'); -%> 

<h1> Compose </h1>

<form action="/compose" method="post" autocomplete="off">
  <div class="form-group">
    <label > Title</label>
    <input class="form-control" type="text" name="postTitle">
    <label > Post</label>
    <textarea class="form-control" id="" cols="30" rows="5" name="postBody"></textarea>

  </div>
  <a class="btn btn-light" href="/">Back</a>
  <button class="btn btn-primary" type="submit"> Publish </button>
</form>  
